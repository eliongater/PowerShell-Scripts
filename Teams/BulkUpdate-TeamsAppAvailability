#Loop through each app, and update AppAssignmentType to "noone"
foreach ($appId in $appIdList) {
    try {
        Update-M365TeamsApp -id $appId -AppAssignmentType "noone"
        Write-Host "Successfully updated app with App ID: $appId to 'noone'" -ForegroundColor Green
    } catch {
        Write-Host "Failed to update app with App ID: $appId. Error: $_" -ForegroundColor Red
    }
}